package br.senai.rn.agenda.model;

import br.senai.rn.agenda.enums.TipoFone;

public class Fone {
	
	private String fone;
	private TipoFone tipoFone;
	
	public Fone() {}

	public Fone(String fone, TipoFone tipoFone) {
		this.fone = fone;
		this.tipoFone = tipoFone;
	}

	public String getFone() {
		return fone;
	}

	public void setFone(String fone) {
		this.fone = fone;
	}

	public TipoFone getTipoFone() {
		return tipoFone;
	}

	public void setTipoFone(TipoFone tipoFone) {
		this.tipoFone = tipoFone;
	}	
	
}
