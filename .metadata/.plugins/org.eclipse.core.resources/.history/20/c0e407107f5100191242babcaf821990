package br.senai.rn.agenda.dao;

import br.senai.rn.agenda.enums.TipoEmail;
import br.senai.rn.agenda.model.Email;

public class TestaEmailDAO {

	public static void main(String[] args) {
		
		EmailDAO dao = new EmailDAO();
		
		dao.salvar(new Email("Miguelbarbosa1309@gmail.com",TipoEmail.PESSOAL));
		dao.salvar(new Email("meutrabalho@gmail.com",TipoEmail.TRABALHO));
		
		System.out.println(dao.buscarTodos());
		
		dao.editar(new Email("1112-3454",TipoEmail.PESSOAL),new Email("1112-3454",TipoEmail.ESCOLA));
		
		System.out.println(dao.buscarTodos());
		
		dao.excluir(new Email("1112-3454",TipoEmail.TRABALHO));
		
		System.out.println(dao.buscarTodos());	
	}
}
