package br.senai.rn.agenda.dao;

import br.senai.rn.agenda.model.Contato;

public class TestaContatoDAO {
	public static void main(String[] args) {
		
		ContatoDAO dao = new ContatoDAO();
		
		dao.salvar(new Contato("Miguel", null, null));
		dao.salvar(new Contato("Marcos", null, null));
		
		System.out.println(dao.buscarTodos());
	}
	
}
