package br.senai.rn.agenda.model;

import br.senai.rn.agenda.enums.TipoEmail;
import br.senai.rn.agenda.enums.TipoFone;
import br.senai.rn.agenda.model.Contato;
import br.senai.rn.agenda.model.Email;
import br.senai.rn.agenda.model.Fone;

public class TestaContato {

	public static void main(String[] args) {
		
		Contato contato = new Contato();
		
		contato.setNome("Miguel");
		contato.adicionarFone(new Fone("4545-6969", TipoFone.FIXO));
		contato.adicionarFone(new Fone("99657-4618", TipoFone.CELULAR));
		contato.adicionarEmail(new Email("miguelbarbosa1309@gmail.com", TipoEmail.PESSOAL));
		
		System.out.println(contato);
		
	}
}
