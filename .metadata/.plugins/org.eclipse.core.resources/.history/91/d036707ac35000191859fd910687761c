package br.senai.rn.agenda.dao;

import java.util.List;

import br.senai.rn.agenda.model.Fone;

public class FoneDAO {

	private static List<Fone> fones;
	
	public void salvar(Fone fone) {
		fones.add(fone);
	}
	
	public void editar(Fone fone1, Fone fone2) {
		fones.add(fone1, fone2);
	}
	
	public void excluir(Fone fone) {
		fones.remove(fone);
	}
	
	public Fone buscar(List<Fone> fone) {
		fones.equals(fone);
		return null;
	}
	
	public List<Fone> buscarTodos() {
		return fones;
	}
}
