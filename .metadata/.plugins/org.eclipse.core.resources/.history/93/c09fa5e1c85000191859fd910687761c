package br.senai.rn.agenda.dao;

import br.senai.rn.agenda.enums.TipoFone;
import br.senai.rn.agenda.model.Fone;

public class TestaContato {

	public static void main(String[] args) {
		
		FoneDAO dao = new FoneDAO();
		
		dao.salvar(new Fone("1112-3454",TipoFone.FIXO));
		dao.salvar(new Fone("3333-4444",TipoFone.FIXO));
		
		System.out.println(dao.buscarTodos());
		
		dao.editar(new Fone("1112-3454",TipoFone.CELULAR));
		
		System.out.println(dao.buscarTodos());
		
		dao.excluir(new Fone("1112-3454",TipoFone.CELULAR));
		
		System.out.println(dao.buscarTodos());
		
		dao.buscar(new Fone("3333-4444"));
		
	}
}
